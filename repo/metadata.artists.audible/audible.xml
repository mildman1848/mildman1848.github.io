<?xml version="1.0" encoding="UTF-8"?>
<scraper framework="1.1" date="2026-02-21">
  <CreateArtistSearchUrl dest="3">
    <RegExp input="$$1" output="&lt;url&gt;https://api.audible.de/1.0/catalog/products?response_groups=contributors,product_desc,product_attrs,series&amp;image_sizes=500&amp;num_results=30&amp;products_sort_by=Relevance&amp;keywords=\1&lt;/url&gt;" dest="3">
      <expression noclean="1">(.+)</expression>
    </RegExp>
  </CreateArtistSearchUrl>

  <GetArtistSearchResults dest="8">
    <RegExp input="$$5" output="&lt;results&gt;\1&lt;/results&gt;" dest="8">
      <RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2&lt;/title&gt;&lt;url&gt;https://api.audible.de/1.0/catalog/contributors/\1?locale=de-DE&lt;/url&gt;&lt;/entity&gt;" dest="5">
        <expression repeat="yes" noclean="1">"authors":\[\{"asin":"([^"]+)","name":"([^"]+)"\}</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\1&lt;/title&gt;&lt;url&gt;https://api.audible.de/1.0/catalog/products?response_groups=contributors,product_desc,product_attrs,series&amp;num_results=20&amp;products_sort_by=Relevance&amp;keywords=\1&lt;/url&gt;&lt;/entity&gt;" dest="5+">
        <expression repeat="yes" noclean="1">"narrators":\[\{"name":"([^"]+)"\}</expression>
      </RegExp>
      <expression noclean="1"/>
    </RegExp>
  </GetArtistSearchResults>

  <GetArtistDetails dest="3">
    <RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
      <RegExp input="$$1" output="&lt;name&gt;\1&lt;/name&gt;" dest="5">
        <expression noclean="1">"name":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;biography&gt;\1&lt;/biography&gt;" dest="5+">
        <expression noclean="1" fixchars="1">"bio":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression noclean="1">"profile_image_url":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression noclean="1">"product_images":\{"1000":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression noclean="1">"product_images":\{(?:"1000":"[^"]+",)?"700":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression noclean="1">"product_images":\{(?:"1000":"[^"]+",)?(?:"700":"[^"]+",)?"500":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression noclean="1">"product_images":\{(?:"1000":"[^"]+",)?(?:"700":"[^"]+",)?(?:"500":"[^"]+",)?"100":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;album&gt;&lt;title&gt;\3&lt;/title&gt;&lt;year&gt;\2&lt;/year&gt;&lt;/album&gt;" dest="5+">
        <expression repeat="yes" noclean="1">"asin":"([^"]+)".*?"release_date":"([0-9]{4})-[0-9]{2}-[0-9]{2}".*?"title":"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="5+">
        <expression repeat="yes" noclean="1">"category_ladders":\[\{"ladder":\[(?:\{"id":"[^"]+","name":"([^"]+)"\},?)+</expression>
      </RegExp>
      <expression noclean="1"/>
    </RegExp>
  </GetArtistDetails>
</scraper>
